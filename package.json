{
  "name": "flatworld",
  "description": "Game engine for 2D strategy games in browsers",
  "version": "0.0.2",
  "main": "src/bundles/fullModuleBundle.js",
  "author": "Janne Hyyti√§",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hachitus/FlaTWorld.git"
  },
  "bugs": {
    "url": "https://github.com/Hachitus/FlaTWorld/issues"
  },
  "homepage": "https://github.com/Hachitus/FlaTWorld",
  "scripts": {
    "start": "echo 'starting instance in http://localhost:9001/' && forever start startEngine.js --uid FlaTWorld",
    "stop": "forever stop startEngine.js FlaTWorld",
    "build": "gulp build",
    "document": "yuidoc -t node_modules/yuidoc-theme-blue",
    "init": "npm i -g forever && ln -s /usr/bin/nodejs /usr/bin/node && npm install && nodejs node_modules/jspm/jspm.js install && nodejs node_modules/bower/lib/bin/bower.js install --allow-root"
  },
  "dependencies": {
    "express": "^4.12.4",
    "handlebars": "^3.0.3",
    "jsonminify": "^0.1.2",
    "loglevel": "^1.3.1",
    "mongoose": "^3.6.20",
    "q": "^1.4.1",
    "validator": "^2.0.0",
    "gulp": "~3.8.10",
    "gulp-rename": "^1.2.0",
    "lodash": "^2.4.1",
    "rimraf": "~2.2.8",
    "watchify": "^2.1.1",
    "vinyl-source-stream": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.5.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babelify": "~5.0.3",
    "browser-sync": "^1.7.1",
    "browserify": "^6.3.3",
    "bower": "^1.7.2",
    "ink-docstrap": "^1.1.0",
    "jscs": "^2.3.0",
    "jshint": "^2.8.0",
    "yuidocjs": "^0.10.0"
  },
  "jspm": {
    "directories": {
      "baseURL": "src"
    },
    "dependencies": {
      "css": "github:systemjs/plugin-css@^0.1.17",
      "hammer": "github:hammerjs/hammer.js@^2.0.6",
      "handlebars": "github:components/handlebars.js@^4.0.5",
      "howler": "npm:howler@^1.1.29",
      "json": "github:systemjs/plugin-json@^0.1.0",
      "loglevel": "npm:loglevel@^1.4.0",
      "q": "npm:q@^1.4.1",
      "text": "github:systemjs/plugin-text@^0.0.2"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
