{
  "source": {
    "include": [ "public/components/" ],
    "exclude": [ "public/components/bundles/coreBundle.js", "public/components/map/core/utils/index.js" ],
    "includePattern": ".+\\.js$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "template": "./node_modules/ink-docstrap/template",
    "encoding": "utf8",
    "destination": "./public/documentation/",
    "recurse": true
  },
  "templates": {
    "systemName"            : "FlaTWorld",
    "copyright"             : "",
    "theme"                 : "cerulean",
    "includeDate"           : true,
    "navType"               : "vertical",
    "linenums"              : true,
    "outputSourceFiles"     : true,
    "outputSourcePath"      : true,
    "syntaxTheme"           : "default",
    "inverseNav"            : true
  }
}